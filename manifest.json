{
  "manifest_version": 2,
  "name": "Upload to Imgur",
  "version": "2.2",
  "minimum_chrome_version": "23",
  "icons": {
    "16": "imgur_16.png",
    "128": "imgur_128.png"
  },
  "permissions" : [
    "contextMenus",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": ["main.js", "jquery.js"]
  },
  "content_scripts" : [
    {
      "matches" : ["http://*/*", "https://*/*"],
      "css" : [],
      "js" : ["jquery.js", "content_script.js"]
    }
  ]
}
